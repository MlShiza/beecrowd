import java.util.Scanner;
public class Main {
    public static void main(String[] args){

        float a, b, media;
        Scanner sc = new Scanner(System.in);
        a = sc.nextFloat();
        b = sc.nextFloat();

        media = (float) ((a* 3.5 + b* 7.5)/11);

        System.out.printf("MEDIA = %.5f\n", media);
    }
}
